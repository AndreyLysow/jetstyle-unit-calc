{
  "name": "jetstyle-unit-calc",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "concurrently \"cross-env NEXT_PUBLIC_API_URL=http://localhost:4000 next dev\" \"ts-node-dev --project tsconfig.server.json --respawn server/index.ts\"",
    "build": "next build && tsc -p tsconfig.server.json",
    "start": "cross-env NEXT_PUBLIC_API_URL=${API_URL:-http://localhost:4000} next start -p ${PORT:-3100}"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "mongoose": "^8.6.0",
    "next": "14.2.5",
    "nodemailer": "^7.0.9",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "sass": "^1.77.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}